ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name example
src_root_dir = /Users/kennetpostigo/Projects/qnd/example
bsc = /Users/kennetpostigo/Projects/qnd/example/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/kennetpostigo/Projects/qnd/example/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I /Users/kennetpostigo/Projects/qnd/example/node_modules/bs-platform/lib/ocaml -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = -I /Users/kennetpostigo/Projects/qnd/example/node_modules/reason-js/lib/ocaml
refmt = /Users/kennetpostigo/Projects/qnd/example/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = -ppx '/Users/kennetpostigo/Projects/qnd/example/node_modules/bs-platform/bin/reactjs_jsx_ppx.exe'
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_deps_from_reason_impl
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/fib.mlast : build_ast_and_deps_from_reason_impl $src_root_dir/src/fib.re
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/fib.mlast.d : build_deps src/fib.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/fib.cmj $src_root_dir/lib/es6/src/fib.js src/fib.cmi : build_cmj_cmi src/fib.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:lib/es6/src
build build.ninja : phony  ||  src/fib.mlast.d
